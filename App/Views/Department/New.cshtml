@model Department
@{
    ViewData["Title"] = "New";
}

<h1 style="text-align:center">New Department</h1>

<form method="post" asp-controller="Department" asp-action="Save">
    <div>
        <label>Name</label>
        <input asp-for="Name" class=" form-control" />
     </div>
    <div>
        <label>Manager Name</label>
        <input asp-for="ManagerName" class=" form-control" />
    </div>
    <input type="submit" value="Add" class="btn btn-success" />
</form>


